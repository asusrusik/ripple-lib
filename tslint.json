{
  "extends": [
    "tslint-eslint-rules"
  ],
  "rules": {
    "ban": [true, ["alert"]], // "no-alert": 2,
    // "no-array-constructor": 2,
    // "no-arrow-condition": 0,
    // "no-bitwise": 0,
    "no-arg": true,
    // "no-case-declarations": 2,
    // "no-catch-shadow": 2,
    // "no-class-assign": 2,
    "no-conditional-assignment": true,
    "no-console": false,
    // "no-const-assign": 2,
    "no-constant-condition": true,
    // "no-continue": 0,
    "no-control-regex": true,
    "no-debugger": true,
    // "no-delete-var": 2,
    // "no-div-regex": 0,
    // "no-dupe-class-members": 2,
    // "no-dupe-keys": 2,
    // "no-dupe-args": 2,
    "no-duplicate-case": true,
    // "no-else-return": 0,
    "no-empty": true,
    "no-empty-character-class": true,
    // "no-empty-pattern": 2,
    // "no-eq-null": 2,
    "no-eval": true,
    "no-ex-assign": true,
    // "no-extend-native": 2,
    // "no-extra-bind": 2,
    "no-extra-boolean-cast": true,
    // "no-extra-parens": [2, "functions"],
    "no-extra-semi": true,
    "no-switch-case-fall-through": true,
    // "no-floating-decimal": 0,
    // "no-func-assign": 2,
    // "no-implicit-coercion": 2,
    // "no-implied-eval": 2,
    // "no-inline-comments": 0,
    "no-inner-declarations": [true, "functions"],
    "no-invalid-regexp": true,
    //  # this rule would cause problems with mocha test cases,
    "no-invalid-this": false,
    "no-irregular-whitespace": true,
    "ter-no-irregular-whitespace": true,
    // "no-iterator": 2,
    // "no-label-var": 2,
    "label-position": true,
    // "no-lone-blocks": 2,
    // "no-lonely-if": 2,
    // "no-loop-func": 2,
    // "no-mixed-requires": [0, false],
    "indent": [true, "spaces", 2],
    "linebreak-style": [true, "unix"],
    "no-multi-spaces": true,
    // "no-multi-str": 2,
    "no-consecutive-blank-lines": [true, 2],
    // "no-native-reassign": 2,
    // "no-negated-condition": 0,
    // "no-negated-in-lhs": 2,
    // "no-nested-ternary": 0,
    "no-unused-expression": true,
    // "no-new-func": 2,
    // "no-new-object": 2,
    // "no-new-require": 0,
    "no-construct": true,
    // "no-obj-calls": 2,
    // "no-octal": 2,
    // "no-octal-escape": 2,
    // "no-param-reassign": 2,
    // "no-path-concat": 0,
    // "no-plusplus": 0,
    // "no-process-env": 0,
    // "no-process-exit": 0,
    // "no-proto": 2,
    "no-duplicate-variable": true,
    "no-regex-spaces": true,
    // "no-restricted-modules": 0,
    // "no-restricted-syntax": 0,
    // "no-return-assign": 2,
    // "no-script-url": 2,
    // "no-self-compare": 2,
    // "no-sequences": 2,
    "no-shadowed-variable": true,
    // "no-shadow-restricted-names": 2,
    // "no-spaced-func": 2,
    "ter-no-sparse-arrays": true,
    // "no-sync": 0,
    // "no-ternary": 0,
    "no-trailing-whitespace": true,
    // "no-this-before-super": 2,
    "no-string-throw": true,
    // "no-undef": 2,
    // "no-undef-init": 2,
    // "no-undefined": 0,
    "no-unexpected-multiline": true,
    // "no-underscore-dangle": 0,
    // "no-unneeded-ternary": 0,
    // "no-unreachable": 2,
    "no-unused-variable": [true, {"ignore-pattern": "^_"}],
    "no-use-before-declare": true,
    // "no-useless-call": 2,
    // "no-useless-concat": 0,
    // "no-void": 2,
    "no-var-keyword": true,
    // "no-warning-comments": [0, {"terms": ["todo", "fixme", "xxx"], "location": "start"}],
    // "no-with": 2,
    "no-magic-numbers": false,

    "array-bracket-spacing": [true, "never"],
    "ter-arrow-body-style": false,
    "ter-arrow-parens": [true, "as-needed"],
    "ter-arrow-spacing": true,
    // "accessor-pairs": 2,
    // "block-scoped-var": 2,
    "block-spacing": true,
    "brace-style": [true, "1tbs", {"allowSingleLine": true}],
    // "callback-return": 0,
    "variable-name": false,
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    // "comma-spacing": 2,
    // "comma-style": 2,
    "cyclomatic-complexity": [false, 11],
    // "ter-computed-property-spacing": true,
    // "consistent-return": 2,
    // "consistent-this": [2, "self"],
    // "constructor-super": 2,
    "curly": [true, "all"],
    "switch-default": false,
    // "dot-location": [2, "property"],
    // "dot-notation": [2, {"allowKeywords": true}],
    "eofline": true,
    "triple-equals": true,
    // "func-names": 0,
    // "func-style": [2, "declaration", {"allowArrowFunctions": true}],
    // "generator-star-spacing": 2,
    // "global-require": 0,
    "forin": false,
    "handle-callback-err": true,
    // "id-length": 0,
    // "init-declarations": 0,
    // "jsx-quotes": 0,
    // "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    // "keyword-spacing": 2,
    // "lines-around-comment": 0,
    // "max-depth": [0, 4],
    "ter-max-len": [true, 80],
    // "max-nested-callbacks": [0, 2],
    // "max-params": [0, 4],
    // "max-statements": [0, 10],
    // "new-cap": 2,
    "new-parens": true,
    // "newline-after-var": 0,
    "object-curly-spacing": [true, "never"],
    "object-literal-shorthand": false,
    "one-variable-per-declaration": [true, "ignore-for-loop"],
    // "operator-assignment": [0, "always"],
    // "operator-linebreak": 0,
    // "padded-blocks": 0,
    "ter-prefer-arrow-callback": false,
    "prefer-const": true,
    // "prefer-spread": 2,
    // "prefer-reflect": 0,
    // "prefer-template": 0,
    "object-literal-key-quotes": false,
    "quotemark": [true, "single"],
    "radix": true,
    // "id-match": 0,
    // "require-jsdoc": 0,
    // "require-yield": 0,
    "semicolon": [true, "never"],
    // "semi-spacing": 2,
    // "sort-vars": 0,
    // "space-before-blocks": 2,
    // "space-before-function-paren": [2, "never"],
    "space-in-parens": [true, "never"],
    // "space-infix-ops": 2,
    // "space-unary-ops": [2, {"words": true, "nonwords": false}],
    "comment-format": [true, "check-space"],
    // "strict": [2, "global"],
    "use-isnan": true,
    // "valid-jsdoc": true,
    "valid-typeof": true,
    // "vars-on-top": 0,
    // "wrap-iife": 0,
    // "wrap-regex": 0,
    // "yoda": [2, "never"],
    "no-constant-condition": true
  }
}